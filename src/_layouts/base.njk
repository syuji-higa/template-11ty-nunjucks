<!doctype html>
<html lang="ja">
<head>
<meta charset="utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width,initial-scale=1.0" />
<title>{{ title }} - {{ common.siteName }}</title>
<meta name="description" content="{{ description }}" />
<link rel="canonical" href="{{ common.siteUrl }}/" />
<link rel="icon" href="{{ common.baseUrl }}/favicon.ico" />
<link rel="apple-touch-icon-precomposed" href="{{ common.baseUrl }}/apple-touch-icon-precomposed.png" />
{% block styles %}{#
#}<link rel="stylesheet" href="/assets/styles/common.css" />
{% endblock %}
</head>
<body>

{% include 'header.njk' %}

<main>
{% block contents %}
{% endblock %}
</main>

{% block scripts %}
<script src="/assets/scripts/common.js"></script>
{% endblock %}

</body>
</html>
